{
  "description": "Sumo Logic logging driver",
  "documentation": "https://github.com/SumoLogic/sumologic-docker-logging-driver",
  "entrypoint": ["/usr/bin/docker-logging-driver"],
  "network": {
    "type": "host"
  },
  "interface": {
    "types": ["docker.logdriver/1.0"],
    "socket": "sumologic.sock"
  }
}
